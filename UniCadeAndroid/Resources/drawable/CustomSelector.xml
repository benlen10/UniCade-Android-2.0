<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:p1="http://schemas.android.com/apk/res/android">
  <item p1:state_pressed="false"
        p1:state_selected="false"
        p1:drawable="@color/cellback" />
  <item p1:state_pressed="true" >
    <shape>
      <gradient
        p1:startColor="#E77A26"
        p1:endColor="#E77A26"
        p1:angle="270" />
    </shape>
  </item>
  <item p1:state_selected="true"
        p1:state_pressed="false"
        p1:drawable="@color/cellback" />
</selector>